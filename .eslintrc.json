{
  "extends": "airbnb",
  "rules": {
    "prefer-destructuring": [
      0
    ],
    "spaced-comment": [
      0
    ],
    "func-names": [
      0
    ],
    "linebreak-style": 0,
    "react/prefer-stateless-function": [
      0,
      {}
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-no-target-blank": ["enabled", { "enforceDynamicLinks": "always" }],
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "class-methods-use-this": [
      0
    ],
    "no-console": 0,
    "no-case-declarations": 0,
    "no-underscore-dangle": [
      0
    ],
    "no-alert": [
      0
    ],
    "no-debugger": 0,
    "import/no-extraneous-dependencies": 0,
    "no-script-url": 0,
    "react/forbid-prop-types": ["enabled", { "forbid": ["string"] }],
    "prettier/prettier": 0,
    "no-plusplus": 0,
    // "no-bitwise": ["error", { "allow": ["<<", "&=", "++", "|"], "int32Hint": true}],
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  },
  "plugins": ["prettier"],
  "globals": {
    "document": true,
    "foo": true,
    "window": true,
    "require": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}